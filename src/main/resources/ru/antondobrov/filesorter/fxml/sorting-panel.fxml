<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" spacing="10.0"
   xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="ru.antondobrov.filesorter.controllers.SortingPanelController">
   <children>
      <ProgressBar fx:id="sortingProgressBar" maxHeight="1.7976931348623157E308"
         maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" progress="0.0"
         HBox.hgrow="ALWAYS" />
      <Button fx:id="startSortingButton" mnemonicParsing="false"
         onAction="#onStartSortingButtonClick" text="%start.sorting.button" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>